<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="EggsInteractiveGUI.MainWindow"
        Title="EggsInteractiveGUI"
        Width="400"
        Height="325">

  <StackPanel Margin="10">
    <TextBox Name="TextBox1" Watermark="Enter first egg count" />
    <TextBox Name="TextBox2" Watermark="Enter second egg count" Margin="0,10,0,0" />
    <TextBox Name="TextBox3" Watermark="Enter second egg count" Margin="0,10,0,0" />
    <TextBox Name="TextBox4" Watermark="Enter second egg count" Margin="0,10,0,0" />
    <TextBox Name="TextBox5" Watermark="Enter second egg count" Margin="0,10,0,0" />
    <Button Name="CalculateButton" Content="Calculate" Margin="0,10,0,0" Click="OnCalculateButtonClick"/>
    <TextBlock Name="ResultLabel" Text="" Margin="0,10,0,0"/>
               
   </StackPanel>
</Window>
